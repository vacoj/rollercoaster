    <add name="ServiceDB" connectionString="Server=localhost;Initial Catalog=YourDataBaseName;Integrated Security=True;" providerName="System.Data.SqlClient" />
  </connectionStrings>
    <appSettings>
      <add key="Identity" value="https://auth.mysite" />
      <add key="MessageBus-C" value="net.tcp://messagebus-a.mysite/mba-ns" />
      <add key="ServiceA" value="tcp://service-a.mysite" />
